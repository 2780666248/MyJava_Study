<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sym.mapper.UserMapper">
    <delete id="delete">
        delete from mall_user where user_id=#{userId}
    </delete>
    <select id="selectAll" resultType="com.sym.pojo.User">
select * from mall_user
    </select>
</mapper>